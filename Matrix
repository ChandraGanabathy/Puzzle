int row=4;int col = 10;
char[,] matrix=new char[row,col];

int alphaNumber = 0; 
for(int i=0; i<row;i++)
{
	for(int j=0; j < col; j++)
	{
		matrix[i,j]=(char)('A' + alphaNumber % 26);
		alphaNumber ++ ;
	}
}

for(int i=0; i<row;i++)
{
	for(int j=0; j < col; j++)
	{
		Console.Write(matrix[i,j] + " ");
	}
	Console.WriteLine();
}
